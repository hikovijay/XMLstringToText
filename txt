switch (cellB.getCellType()) {
                        case CellType.STRING:
                            String valueB = cellB.getStringCellValue();
                            handleStringDataType(valueB, row, sheet);
                            break;
                        case CellType.NUMERIC:
                            double numericValue = cellB.getNumericCellValue();
                            handleNumericDataType(numericValue, row, sheet);
                            break;
                        case CellType.BOOLEAN:
                            boolean booleanValue = cellB.getBooleanCellValue();
                            handleBooleanDataType(booleanValue, row, sheet);
                            break;
                        // Add additional cases for other data types as needed
                        default:
                            // Handle unsupported data types or empty cells
                            break;
                    }
