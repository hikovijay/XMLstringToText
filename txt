for (int i = 0; i < stringList.size(); i++) {
                Row row = sheet.getRow(startRow + i);
                if (row == null) {
                    // If the row doesn't exist, create it
                    row = sheet.createRow(startRow + i);
                }

                Cell cell = row.createCell(targetColumn);
                cell.setCellValue(stringList.get(i));
            }
