import java.util.ArrayList;
import java.util.List;

public class SplitStringList {
    public static void main(String[] args) {
        // Create a list with two string values
        List<String> stringList = new ArrayList<>();
        stringList.add("Value1");
        stringList.add("Value2");

        // Access the elements and store them in separate variables
        if (stringList.size() >= 2) {
            String value1 = stringList.get(0);
            String value2 = stringList.get(1);

            System.out.println("Value1: " + value1);
            System.out.println("Value2: " + value2);
        } else {
            System.out.println("The list does not contain at least two values.");
        }
    }
}
