public class FileNameModifier {
    public static void main(String[] args) {
        String fileName = "example.xlsx";
        
        // Remove the ".xlsx" extension
        String fileNameWithoutExtension = removeFileExtension(fileName);
        
        System.out.println("Original File Name: " + fileName);
        System.out.println("File Name without Extension: " + fileNameWithoutExtension);
    }
    
    public static String removeFileExtension(String fileName) {
        if (fileName != null && fileName.contains(".")) {
            return fileName.substring(0, fileName.lastIndexOf('.'));
        }
        return fileName; // Return the original string if there is no extension.
    }
}
