public static void closeWorkbooks() {
    for (Map.Entry<String, XSSFWorkbook> entry : workbooks.entrySet()) {
        try {
            String excelFilePath = entry.getKey();
            XSSFWorkbook workbook = entry.getValue();
            
            FileOutputStream fos = new FileOutputStream(excelFilePath);
            workbook.write(fos);
            fos.close();
        } catch (Exception e) {
            e.printStackTrace();
        }
    }
}
